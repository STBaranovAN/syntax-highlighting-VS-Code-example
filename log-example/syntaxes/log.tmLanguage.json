{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "log",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
					"name": "constant.regexp.log",
					"match": "(LOG)"
				}, 
				{
					"name": "comment.log",
					"match": "(--SUC--)"
				},
				{
					"name": "invalid.log",
					"match": "(--ERR--)"
				}
			]
		},
		"strings": {
			"name": "string.quoted.double.log",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.log",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.log"
}