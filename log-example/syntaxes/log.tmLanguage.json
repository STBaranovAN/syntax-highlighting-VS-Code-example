{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "log",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#date"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
					"name": "constant.regexp.log",
					"match": "(LOG)"
				}, 
				{
					"name": "comment.log",
					"match": "(--SUC--)"
				},
				{
					"name": "invalid.log",
					"match": "(--ERR--)"
				}
			]
		},
		"date": {
			"patterns": [{
					"match": "([0-9]{4}-[0-9]{2}-[0-9]{2})( - )([0-9]{2}:[0-9]{2}:[0-9]{2})",
					"captures": {
						"1": {
							"name": "markup.changed.log"
						},
						"3": {
							"name": "markup.changed.log"
						}
					}
				}
			]
		},
		"strings": {
			"name": "string.quoted.double.log",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.log",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.log"
}